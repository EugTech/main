{
   "containerDefinitions": [ 
      { 
         "essential": true,
         "image": "937163516619.dkr.ecr.us-west-2.amazonaws.com/museum-adventure:v1",
         "logConfiguration": { 
            "logDriver": "awslogs",
            "options": { 
               "awslogs-group" : "/ecs/museum-adventure",
               "awslogs-region": "us-west-2",
               "awslogs-stream-prefix": "ecs"
            }
         },
         "name": "museum-adventure",
         "portMappings": [ 
            { 
               "containerPort": 8080,
               "hostPort": 8080,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::012345678910:role/ecsTaskExecutionRole",
   "family": "museum-adventure",
   "memory": "512",
   "networkMode": "awsvpc",
   "requiresCompatibilities": [ 
       "FARGATE" 
    ]
}
